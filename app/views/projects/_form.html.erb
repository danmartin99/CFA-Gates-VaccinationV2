<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :vacc_type %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :batch_number %>
    <%= f.association :vaccinator, label_method: :email, value_method: :id, include_blank: false %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
